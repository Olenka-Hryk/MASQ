                <lvc:CartesianChart x:Name="Cartesian" Width="600" Height="0" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" Series="{Binding SeriesCollection}" LegendLocation="Left">
                    <lvc:CartesianChart.AxisX>
                        <lvc:Axis Title="Метрики якості ПЗ" Labels="{Binding Labels}"></lvc:Axis>
                    </lvc:CartesianChart.AxisX>
                    <lvc:CartesianChart.AxisY>
                        <lvc:Axis Title="Значення метрик" LabelFormatter="{Binding Formatter}"></lvc:Axis>
                    </lvc:CartesianChart.AxisY>
                </lvc:CartesianChart>
                <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->
                <!--PIE CHART-->
                <lvc:PieChart x:Name="Pie" LegendLocation="Right" Grid.Row="6" Width="650" Height="0" Grid.Column="0" Grid.ColumnSpan="3" DataClick="ChartPie_OnDataClick" Hoverable="False" Margin="0,0,20,0" DataTooltip="{x:Null}">
                </lvc:PieChart>
                <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->
            </Grid>
        </ScrollViewer>



        <!--==============================================================================================-->
        <!-- Дизайнерські фічі :) -->
        <!--==============================================================================================-->
        <Rectangle Fill="#FF3030BB" HorizontalAlignment="Left"  Height="22" MaxHeight="23" Margin="3,89,0,0" Stroke="Black" VerticalAlignment="Top" Width="353" MinWidth="310" />
        <Label Content="Множина метрик якості ПЗ" Margin="67,84,62,0" Height="27" Foreground="#FFF5F3F3" FontWeight="Bold" FontFamily="Calisto MT" FontSize="16" VerticalAlignment="Top"/>
        <Rectangle Fill="#FF3030BB" HorizontalAlignment="Left" Height="22" MaxHeight="23" Margin="0,89,9,0" Stroke="Black" VerticalAlignment="Top" MinWidth="650" Width="{Binding ElementName=grid_Main, Path=ActualWidth}" Grid.Column="2" />
        <Label x:Name="Title" Content="Розрахування метрик" Grid.Column="2" HorizontalAlignment="Left" Margin="300,84,0,0"  Foreground="#FFF5F3F3" FontWeight="Bold" FontFamily="Calisto MT" FontSize="16" VerticalAlignment="Top" Height="27" Width="279"/>
        <Rectangle Fill="#FF3030BB" HorizontalAlignment="Left" Height="22" MaxHeight="23" Margin="0,5,9,0" Stroke="Black" VerticalAlignment="Top" MinWidth="650" Width="{Binding ElementName=grid_Main, Path=ActualWidth}" Grid.Row="3"  Grid.Column="2" />
        <Label Content="Аналіз метрик" Grid.Row="3" Grid.Column="2" HorizontalAlignment="Left" Margin="300,2,0,5"  Foreground="#FFF5F3F3" FontWeight="Bold" FontFamily="Calisto MT" FontSize="16" Height="27" Width="237" HorizontalContentAlignment="Stretch" VerticalAlignment="Top"/>
        <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->

    </Grid>
</Window>