<Window x:Class="MetricAnalyzerSoftwareQuality.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MetricAnalyzerSoftwareQuality"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MASQ 1.0                                                                                                                                                             Metric Analyzer Software Quality" Height="700" MinHeight="700" Width="1050" MinWidth="1050" Icon="Images/icon_main.png"
        ResizeMode="CanResizeWithGrip" Background="White" WindowStyle="ThreeDBorderWindow" KeyDown="ControlAllHotKey_KeyDown" WindowState="Maximized"
        x:Name="thisWindow">

    <Grid Name="grid_Main" Background="#FF6D8CF5">
        <Grid.RowDefinitions>
            <RowDefinition Height="111"/>
            <RowDefinition Height="456*"/>
            <RowDefinition Height="155"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="357"/>
            <ColumnDefinition Width="7"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <!--==============================================================================================-->
        <!-- Створення головного меню -->
        <!--==============================================================================================-->
        <Menu Height="25" MinHeight="25" MinWidth="600" VerticalAlignment="Top" Grid.ColumnSpan="3">
            <Menu.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionColorKey}}"/>
            </Menu.Background>

            <!-- Меню ФАЙЛ -->
            <MenuItem Header="Файл" Name="Menu_file" FontSize="15" FontFamily="Calibri" Foreground="#FF141F8B" HorizontalAlignment="Center" VerticalAlignment="Center">
                <MenuItem  Header="Створити" Name="Menu_file_new" Cursor="Hand" InputGestureText="Ctrl+N" Click="New_forStart_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_new.png" />
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="Зберегти" Name="Menu_file_save" Cursor="Hand" InputGestureText="Ctrl+S" Click="Save_all_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_save.png" />
                    </MenuItem.Icon>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Налаштування" Name="Menu_file_settings" Cursor="Hand" InputGestureText="F6" Click="Settings_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_settings.png" />
                    </MenuItem.Icon>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Вихід" Name="Menu_file_exit" Cursor="Hand" InputGestureText="F4" Click="Exit_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_exit.png" />
                    </MenuItem.Icon>
                </MenuItem>
            </MenuItem>

            <!-- Меню ОСНОВНЕ -->
            <MenuItem Header="Основне" Name="Menu_general" FontSize="15" FontFamily="Calibri" Foreground="#FF141F8B" HorizontalAlignment="Center" VerticalAlignment="Center">
                <MenuItem Header="Оновити параметри" Name="Menu_general_refresh" Cursor="Hand" InputGestureText="Alt+R" Click="Refresh_allParameters_Click" >
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_refresh.png" />
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="Очистити параметри" Name="Menu_general_clean" Cursor="Hand" InputGestureText="Ctrl+del" Click="Clean_allParameters_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_clean_empty.png" />
                    </MenuItem.Icon>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Переглянути..." Name="Menu_general_view" Cursor="Hand">
                    <MenuItem Header="всі параметри" Name="Menu_general_view_parametrs" Cursor="Hand" InputGestureText="Alt+A" Click="View_allParameters_Click"></MenuItem>
                    <MenuItem Header="всі метрики" Name="Menu_general_view_metrics" Cursor="Hand" InputGestureText="Alt+M" Click="View_allMetrics_Click"></MenuItem>
                    <MenuItem.Icon>
                        <Image Source="Images/icon_all_metrics.png" />
                    </MenuItem.Icon>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Зберегти" Name="Menu_general_save" Cursor="Hand" InputGestureText="Ctrl+S" Click="Save_all_Click" >
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_save.png" />
                    </MenuItem.Icon>
                </MenuItem>
            </MenuItem>

            <!-- Меню ПОШУК -->
            <MenuItem Header="Пошук" Name="Menu_search" FontSize="15" FontFamily="Calibri" Foreground="#FF141F8B" HorizontalAlignment="Center" VerticalAlignment="Center">
                <MenuItem Header="Знайти параметри" Name="Menu_search_parametrs" Cursor="Hand" InputGestureText="Alt+F+P" Click="Search_parameter_Click"/>
                <MenuItem Header="Знайти метрики" Name="Menu_search_metrics" Cursor="Hand" InputGestureText="Alt+F+M" Click="Search_metric_Click"/>
            </MenuItem>

            <!-- Меню ЗВІТИ -->
            <MenuItem Header="Звіти" Name="Menu_zvit"  FontSize="15" FontFamily="Calibri" Foreground="#FF141F8B" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Report_Click"/>

            <!-- Меню НАЛАШТУВАННЯ -->
            <MenuItem Header="Налаштування" Name="Menu_settings" FontSize="15" FontFamily="Calibri" Foreground="#FF141F8B" HorizontalAlignment="Center" VerticalAlignment="Center">
                <MenuItem Header="Персоналізація..." Name="Menu_settings_personalization" Cursor="Hand" InputGestureText="F8" Click="Settings_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/icon_personalization.png" />
                    </MenuItem.Icon>
                </MenuItem>
            </MenuItem>

            <!-- Меню ІНФОРМАЦІЯ -->
            <MenuItem Header="Інформація" Name="Menu_information" FontSize="15" FontFamily="Calibri" Foreground="#FF141F8B" HorizontalAlignment="Center" VerticalAlignment="Center">
                <MenuItem Header="Про програму" Name="Menu_information_program" Cursor="Hand" Click="Information_aboutProgram_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_information.png" />
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="Про автора" Name="Menu_information_author" Cursor="Hand" Click="Information_aboutAuthor_Click">
                    <MenuItem.Icon>
                        <Image Source="Images/icon_info_author.png" />
                    </MenuItem.Icon>
                </MenuItem>
            </MenuItem>

            <!-- Меню ДОПОМОГА -->
            <MenuItem Header="Допомога" Name="Menu_help" FontSize="15" FontFamily="Calibri" Foreground="#FF141F8B" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Help_Click">
                <MenuItem Header="Переглянути довідку" Name="Menu_help_view" Cursor="Hand" InputGestureText="F1">
                    <MenuItem.Icon>
                        <Image Source="Images/IconMenu/icon_questions.png" />
                    </MenuItem.Icon>
                </MenuItem>
            </MenuItem>
        </Menu>
        <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->



        <!--==============================================================================================-->
        <!-- Створення панелі інструментів -->
        <!--==============================================================================================-->
        <ToolBarTray x:Name="ToolBar" HorizontalAlignment="Left" Height="59" Margin="0,25,0,0" VerticalAlignment="Top" Width="{Binding ElementName=grid_Main, Path=ActualWidth}" Grid.ColumnSpan="3">
            <ToolBar>
                <Button x:Name="New_tb_button" ToolTip="Створити" Height="59" Margin="0,0,0,75" Width="73" Click="New_forStart_Click">
                    <Image Source="Images/IconMenu/icon_new.png"/>
                </Button>
                <Button x:Name="Refresh_tb_button" ToolTip="Оновити параметри" Height="59" Margin="0,0,0,75" Width="73" Click="Refresh_allParameters_Click">
                    <Image Source="Images/IconMenu/icon_refresh.png"/>
                </Button>
                <Button x:Name="Clean_tb_button" ToolTip="Очистити параметри" Height="59" Margin="0,0,0,75" Width="73" Click="Clean_allParameters_Click">
                    <Image Source="Images/IconMenu/icon_clean_empty.png"/>
                </Button>
                <Separator/>
                <Button x:Name="Search_tb_button" ToolTip="Знайти параметр/метрику" Height="59" Margin="0,0,0,75" Width="73" Click="Search_parameter_OR_metric_Click">
                    <Image Source="Images/icon_search.png"/>
                </Button>
                <Button x:Name="Report_tb_button" ToolTip="Звіти" Height="59" Margin="0,0,0,75" Width="73" Click="Report_Click">
                    <Image Source="Images/IconMenu/icon_report.png"/>
                </Button>
                <Button x:Name="Save_tb_button" ToolTip="Зберегти" Height="59" Margin="0,0,0,75" Width="73" Click="Save_all_Click">
                    <Image Source="Images/IconMenu/icon_save.png"/>
                </Button>
                <Separator/>
                <Button x:Name="AllParametrs_tb_button" ToolTip="Переглянути всі параметри" Height="59" Margin="0,0,0,75" Width="73" Click="View_allParameters_Click">
                    <Image Source="Images/IconMenu/icon_view_all_parametrs.png"/>
                </Button>
                <Button x:Name="AllMetrics_tb_button" ToolTip="Переглянути всі метрики" Height="59" Margin="0,0,0,75" Width="73" Click="View_allMetrics_Click">
                    <Image Source="Images/IconMenu/icon_view_all_metrics.png"/>
                </Button>
                <Separator/>
                <Button x:Name="Settings_tb_Button" ToolTip="Налаштування" Height="59" Margin="0,0,0,75" Width="73" Click="Settings_Click">
                    <Image Source="Images/IconMenu/icon_settings.png"/>
                </Button>
                <Button x:Name="Information_tb_button" ToolTip="Інформація" Height="59" Margin="0,0,0,75" Width="73" Click="Information_aboutProgram_Click">
                    <Image Source="Images/IconMenu/icon_information.png"/>
                </Button>
                <Button x:Name="Help_tb_button" ToolTip="Допомога" Height="59" Margin="0,0,0,75" Width="73" Click="Help_Click">
                    <Image Source="Images/IconMenu/icon_questions.png"/>
                </Button>
                <Separator/>
                <Button x:Name="Exit_tb_button" ToolTip="Вихід" Height="59" Margin="0,0,0,75" Width="73" Click="Exit_Click">
                    <Image Source="Images/IconMenu/icon_exit.png"/>
                </Button>
            </ToolBar>
        </ToolBarTray>
        <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->



        <!--==============================================================================================-->
        <!-- Створення дерева перегляду всіх метрик -->
        <!--==============================================================================================-->
        <TreeView x:Name="ListOfAllMetrics_tv"  HorizontalAlignment="Left" Margin="3,110,0,25" Width="353" MinWidth="310" Cursor="Hand" BorderBrush="#FF203DF1" ScrollViewer.HorizontalScrollBarVisibility="Visible" Grid.RowSpan="3" SelectedItemChanged="ListOfAllMetrics_SelectedItemChanged">
            <TreeView.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFF3F6F7" Offset="0"/>
                    <GradientStop Color="#FFC2D3DA" Offset="0.675"/>
                </LinearGradientBrush>
            </TreeView.Background>

            <!-- Метрики з ТОЧНИМИ ЗНАЧЕННЯМИ-->
            <TreeViewItem x:Name="Metricts_with_exact_value_tvi" IsExpanded="True" Cursor="Hand">
                <TreeViewItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Height="25" Source="Images/icon_all_metrics.png" Width="25" />
                        <TextBlock Margin="5,0" Text="  Метрики з точними значеннями" Foreground="#FF444497" FontWeight="Bold" FontStyle="Italic" FontSize="14" />
                    </StackPanel>
                </TreeViewItem.Header>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика зв'язності" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика зчеплення" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика звернення до глобальної змінної" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика часу модифікації моделей" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика загальної кількості знайдених помилок при інспектуванні моделей та прототипів підсистем, модулів, функцій, вимог та густота помилок" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
            </TreeViewItem>
            <Separator/>

            <!-- Метрики з ПРОГНОЗОВАНИМИ ЗНАЧЕННЯМИ-->
            <TreeViewItem  x:Name="Metricts_with_predicted_value_tvi" IsExpanded="True" Cursor="Hand">
                <TreeViewItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Height="25" Source="Images/icon_all_metrics.png" Width="25" />
                        <TextBlock Margin="5,0" Text="  Метрики з прогнозованими значеннями" FontSize="14" FontWeight="Bold" FontStyle="Italic" Foreground="#FF444497"/>
                    </StackPanel>
                </TreeViewItem.Header>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика прогнозованого загального часу розроблення ПЗ" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика часу виконання робіт етапу проектування ПЗ" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика очікуваної вартості розроблення ПЗ" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика прогнозованої вартості перевірки якості ПЗ" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика прогнозованої продуктивності розроблення ПЗ" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика прогнозованих витрат на реалізацію програмного коду" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика прогнозованого функційного розміру" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика прогнозованої оцінки трудовитрат за моделю Боема" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
                <TreeViewItem>
                    <TreeViewItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="20" Source="Images/icon_metrics.png" Width="20" />
                            <TextBlock Margin="5,0" Text="Метрика прогнозованої оцінки тривалості проекту за моделю Боема" Foreground="#FF27278F" />
                        </StackPanel>
                    </TreeViewItem.Header>
                </TreeViewItem>
            </TreeViewItem>
        </TreeView>
        <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->



        <!--==============================================================================================-->
        <!-- Створення TabControl для перегляду всіх метрик -->
        <!--==============================================================================================-->
        <TabControl x:Name="AllMetrics_TabControl" Visibility="Hidden" Grid.Column="2" Margin="0,0,9,0" MinHeight="404" Height="{Binding ElementName=grid_Main, Path=ActualHeight}" Foreground="#FF444497" Grid.Row="1" Width="{Binding ElementName=grid_Main, Path=ActualWidth}" RenderTransformOrigin="0.5,0.59" VerticalAlignment="Top" HorizontalAlignment="Left" FontFamily="Bookman Old Style" FontSize="18" FontWeight="Bold" SelectionChanged="AllMetrics_TabControl_SelectionChanged" VerticalContentAlignment="Top">
            <TabControl.Background>
                <RadialGradientBrush>
                    <GradientStop Color="#FFDBBDF0" Offset="0"/>
                    <GradientStop Color="#FFDCCCCC" Offset="1"/>
                    <GradientStop Color="#FF4E7DC1" Offset="0.454"/>
                </RadialGradientBrush>
            </TabControl.Background>

            <!--********************************************************************************************-->
            <!-- C H P -->
            <!--********************************************************************************************-->
            <TabItem x:Name="Chp_TabItem" Header="Chp" Cursor="Hand" Foreground="#FF444497">
                <TabItem.Background>
                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                        <GradientStop Color="#FFF0F0F0" Offset="0"/>
                        <GradientStop Color="#FFA6C3F3" Offset="1"/>
                    </LinearGradientBrush>
                </TabItem.Background>
                <!-- Створення Grid та ScrollViewer для нього -->
                <ScrollViewer x:Name="CHP_ScrollViewerForThisTab" VerticalAlignment="Top" Height="Auto" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" CanContentScroll="True" PanningMode="VerticalOnly" SnapsToDevicePixels="True" Background="Transparent" PreviewMouseWheel="ScrollViewerForThisTab_PreviewMouseWheel" Margin="0,0,0,262">
                    <Grid x:Name="gridForTabArea_CHP" VerticalAlignment="Top" HorizontalAlignment="Left" Height="Auto">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="510*"/>
                            <ColumnDefinition Width="72"/>
                            <ColumnDefinition Width="85"/>
                            <ColumnDefinition Width="7"/>
                            <ColumnDefinition Width="362"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="74"/>
                            <RowDefinition Height="1"/>
                            <RowDefinition Height="220"/>
                            <RowDefinition Height="87*"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.Background>
                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.InactiveBorderColorKey}}"/>
                        </Grid.Background>

                        <!-- <<<<<<<<<<<<<<<<<<<<<<< ОСНОВНІ ЕЛЕМЕНТИ ДЛЯ TabItem >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->
                        <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->
                        <Label Content="Метрика зв'язності" MinWidth="582" Width="{Binding ElementName=grid_Main, Path=ActualWidth}" Margin="-235,0,229,0" Height="74" VerticalAlignment="Center" FontFamily="Georgia" FontStyle="Italic" FontSize="24" Background="{x:Null}" Foreground="#FF575BAC" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Grid.ColumnSpan="5" VerticalContentAlignment="Center"/>
                        <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->
                        <Image Source="Images/icon_metrics.png" Grid.Column="1" HorizontalAlignment="Left" Height="55" Grid.Row="3" VerticalAlignment="Top" Width="62" Margin="10,10,0,0"/>
                        <Label x:Name="ResultAnalysisMetric_CHP_lb" Content="0" Grid.Column="2" HorizontalAlignment="Left" Grid.Row="3" Width="75" Height="55" Background="{x:Null}" Foreground="#FF3C47B6" VerticalAlignment="Top" VerticalContentAlignment="Center" FontStyle="Italic" FontSize="36" FontFamily="Century Schoolbook" Margin="0,10,0,0"/>
                        <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->


                        <!-- <<<<<<<<<<<<<<<<<<<<<<<< ОСНОВНІ КНОПКИ  ДЛЯ TabItem >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->
                        <!-- Кнопка ІНФОРМАЦІЇ ПРО МЕТРИКУ -->
                        <Button x:Name="InformationForMetric_CHP_btn" Click="ShowInformationForThisMetric_Click" HorizontalAlignment="Left" ToolTip="Додаткова інформація про метрику" VerticalAlignment="Top" Width="70" Height="74" BorderBrush="{x:Null}" Foreground="{x:Null}" Cursor="Help" FontWeight="Bold" Background="{x:Null}" Grid.Column="2">
                            <Button.Content>
                                <StackPanel Height="64">
                                    <Image Source="Images/IconMenu/icon_information.png"/>
                                </StackPanel>
                            </Button.Content>
                        </Button>
                        <!-- Кнопка ОБЧИСЛЮВАЛЬНОЇ ІНФОРМАЦІЇ ПРО МЕТРИКУ -->
                        <Button x:Name="CalculationForMetric_CHP_btn" Click="ShowCalculationInformationForThisMetric_Click" Visibility="Collapsed" HorizontalAlignment="Left" ToolTip="Обчислення метрики" Grid.Row="3" VerticalAlignment="Top" Width="70" Height="74" BorderBrush="{x:Null}" Foreground="{x:Null}" Cursor="Help" FontWeight="Bold" Background="{x:Null}">
                            <Button.Content>
                                <StackPanel Height="64">
                                    <Image Source="Images/IconMenu/icon_remark.png"/>
                                </StackPanel>
                            </Button.Content>
                        </Button>
                        <!-- Кнопка ЗАДАТИ -->
                        <Button x:Name="NewParametrs_CHP_btn" Click="NewParametrsForThisMetric_Click" Grid.ColumnSpan="2" Content="Задати" Grid.Column="1" HorizontalAlignment="Left" Height="34" Grid.Row="2" VerticalAlignment="Top" Width="100" FontSize="18" FontWeight="Normal" FontFamily="Impact" Foreground="#FF403F5F" BorderBrush="{x:Null}" Margin="47,0,0,0">
                            <Button.Background>
                                <ImageBrush ImageSource="Images/IconButton/btn_new.png"/>
                            </Button.Background>
                        </Button>
                        <!-- Кнопка ОНОВИТИ -->
                        <Button x:Name="RefreshParametrs_CHP_btn" Visibility="Hidden" Click="RefreshParametrsForThisMetric_Click" Grid.ColumnSpan="2" Content="Оновити" Grid.Column="1" HorizontalAlignment="Left" Height="34" Margin="47,39,0,0" Grid.Row="2" VerticalAlignment="Top" Width="100" FontWeight="Normal" FontFamily="Impact" Foreground="#FF403F5F" BorderBrush="{x:Null}">
                            <Button.Background>
                                <ImageBrush ImageSource="Images/IconButton/btn_refresh.png"/>
                            </Button.Background>
                        </Button>
                        <!-- Кнопка ОЧИСТИТИ -->
                        <Button x:Name="CleanParametrs_CHP_btn" Visibility="Hidden" Click="CleanParametrsForThisMetric_Click" Grid.ColumnSpan="2" Content="Очистити" Grid.Column="1" HorizontalAlignment="Left" Height="34" Margin="47,78,0,0" Grid.Row="2" VerticalAlignment="Top" Width="100" FontWeight="Normal" FontFamily="Impact" Foreground="#FF403F5F" BorderBrush="{x:Null}">
                            <Button.Background>
